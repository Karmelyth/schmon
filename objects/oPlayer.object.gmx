        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Resetting vars:
nearest = -4;

//Controls:
if g.can_control = 0{
    if keyboard_check(g.key_left){
        motion_add(180, 1);
    }
    if keyboard_check(g.key_right){
        motion_add(0, 1);
    }
    if keyboard_check(g.key_up){
        motion_add(90, 1);
    }
    if keyboard_check(g.key_down){
        motion_add(270, 1);
    }
    
    with oMapObject{ //interactives sorting
    var _c = false; //check if theres ANY valid instance
        if can_interact = true{
            if distance_to_object(other) &lt;= 12{
                _c = true;
                with oPlayer{
                    if nearest &gt; -4{
                       if point_distance(mouse_x, mouse_y, other.x, other.y) &lt; point_distance(mouse_x, mouse_y, nearest.x, nearest.y){
                            nearest = other;
                       }
                   }else{
                    nearest = other;
                   }
                }
            }
        }
    }
    if instance_exists(nearest){
        if keyboard_check_pressed(g.key_interact){
            script_execute(nearest.interaction);
            g.can_control++;
            g.interaction = nearest;
        }
    }
}



mob_finish();

//Collision:
if place_meeting(x + hspeed, y, oWall) || place_meeting(x + hspeed, y, oNPC){
    hspeed = 0;
}
if place_meeting(x, y + vspeed, oWall) || place_meeting(x, y + vspeed, oNPC){
    vspeed = 0;
}
if place_meeting(x + hspeed, y + vspeed, oWall) || place_meeting(x + hspeed, y + vspeed, oNPC){
    hspeed = 0;
    vspeed = 0;
}
</string>
