            <string>//Camera:
var _weight_total = 5,
    _xx = x * 4,
    _yy = y * 4;

//adding mouse coords:
_xx += mouse_x * .075;
_yy += mouse_y * .075;
_weight_total += .075;

    
//adding interaction instances coords:
if interaction &gt; noone{
    _xx += interaction.x * 1.5;
    _yy += interaction.y * 1.5;
    _weight_total += 1.5;
}

if in_pause = 0{  
    x = (_xx + view_instance.x + view_instance.hspeed)/_weight_total;
    y = (_yy + view_instance.y + view_instance.vspeed)/_weight_total;
}

//Pause menu:
if keyboard_check_pressed(key_pause) &amp;&amp; can_control = false{
    if in_pause = false{
        instance_deactivate_all(true);
        in_pause = true;
        
        //Pause menu buttons:
        instance_create(0, 0, oPauseManager);
        
    }else{
        in_pause = false;
        instance_destroy(oPauseManager);
        instance_activate_all();
    }
}

//Pause action:
if in_pause = true{
    if !sprite_exists(temp_surface){ 
        temp_surface = sprite_create_from_surface(application_surface, 0, 0, surface_get_width(application_surface), surface_get_height(application_surface), 0, 0, 0, 0);   
    }
}else{
    if sprite_exists(temp_surface){
        sprite_delete(temp_surface);
    }
}
